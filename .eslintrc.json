{
  "extends": [
    "prettier", 
    "prettier/react",
    "eslint:recommended", 
    "plugin:prettier/recommended", 
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-undef": "warn",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
